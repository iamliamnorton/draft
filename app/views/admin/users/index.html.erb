<div class="col-md-12">
  <div class="container">
    <div class="row">
      <div class="col-md-12">
        <div class="panel panel-default">
          <div class="panel-heading">
            <h3 class="panel-title">Users</h3>
          </div>
          <div class="panel-body">
            <table class="table table-condensed">
              <tr>
                <th>
                  Email
                </th>
                <th>
                  Credit
                </th>
                <th>
                  Contests
                </th>
                <th>
                  Entries
                </th>
                <th>
                  Win %
                </th>
                <th>
                </th>
              </tr>
              <% @users.each do |user| %>
                <tr>
                  <th>
                    <%= user.email %>
                  </th>
                  <th>
                    <%= user.credit %>
                  </th>
                  <th>
                    <%= user.contests.count %>
                  </th>
                  <th>
                    <%= user.entries.count %>
                  </th>
                  <th>
                    <%= "na" %>
                  </th>
                  <th>
                  </th>
                </tr>
              <% end %>
            </table>
          </div>
        </div>
      </div>
    </div>
  </div>
</div>
